    "vite-plugin-pwa": "^0.20.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-cropper": "^0.6.5",
    "vue-tsc": "^2.0.21",
    "vue3-cookies": "^1.0.6",
    "vue3-lottie": "^3.3.0",
    "workbox-window": "^7.1.0",
    "xlsx": "^0.18.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
