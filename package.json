    "hao-admin",
    "element-plus",
    "tailwindcss",
    "typescript",
    "pinia",
    "vue3",
    "vite"
  ],
  "homepage": "https://github.com/a596116/hao-admin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a596116/hao-admin.git"
  },
  "bugs": {
    "url": "https://github.com/a596116/hao-admin/issues"
  },
  "license": "MIT",
  "author": {
    "name": "a596116",
    "email": "a596116@gmail.com",
